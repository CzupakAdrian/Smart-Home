{
    "configurations": [
        {
            "name": "Linux",
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${workspaceFolder}/extern/googletest/googletest/include/**",
                "${workspaceFolder}/extern/googletest/googlemock/include/**",
                "${workspaceFolder}/include/**"
            ],
            "compilerPath": "/usr/bin/clang++",
            "compilerArgs": [
                "-Wall",
                "-pedantic",
                "-Wextra",
                "-Werror",
                "-frtti"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.cmake-tools"
            ,
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}