{
    "configurations": [
        {
            "name": "Linux",
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${workspaceFolder}/googletest/googletest/include",
                "${workspaceFolder}/googletest/googlemock/include",
                "${workspaceFolder}"
            ],
            "compilerPath": "/usr/bin/clang++",
            //"configurationProvider": "vector-of-bool.cmake-tools",
            "compilerArgs": [
                "-Wall",
                "-pedantic",
                "-Wextra",
                "-Werror",
                "-frtti"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}